<%= link_to "Home", root_path %>
<%= link_to "Bloggers", users_path %>
<%= link_to "Categories" %>
<%= link_to "Blogs" %>

<% unless logged_in? %>
<%= button_to "Login", login_path, method: :get %>
<%= button_to "Sign up", new_user_path, method: :get %>
<% else %>
<%= form_with url: logout_path, method: :delete do %>
  <%= submit_tag "Logout"%>
<% end %>
<% end %>
